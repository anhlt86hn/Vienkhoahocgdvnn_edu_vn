@model PagedList.IPagedList<VKHGDDTNN.Models.Contact>
@{
    ViewBag.Title = "Nhóm bài viết";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
@section Styles
{
    <!--Sử dụng cho Confirm delete-->
    <link href="~/Content/jquery-ui.css" rel="stylesheet" />
}

@section Scripts
{
    <!-- Ajax sử dụng cho phân trang  -->
    <script src="~/Scripts/jquery.unobtrusive-ajax.js"></script>

    <!--Sử dụng cho Confirm delete-->
    <script src="~/Scripts/jquery-1.9.1.js"></script>
    <script src="~/Scripts/jquery-ui.js"></script>
    <script src="~/Scripts/jquery.easy-confirm-dialog.js"></script>
}
<!-- MultipleDelete -->
<script type="text/javascript">
    $(document).ready(function () {
        $(".deletemultiple").easyconfirm({
            locale: { title: 'Xóa Phản hồi/liên hệ', button: ['Hủy', 'Xóa'] }
        });

    })
</script>




